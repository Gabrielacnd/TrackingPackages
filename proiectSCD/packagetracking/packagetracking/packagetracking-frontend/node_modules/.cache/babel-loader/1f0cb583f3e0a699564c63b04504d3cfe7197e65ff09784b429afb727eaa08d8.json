{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"AddPackage\",\n  props: {\n    currentUser: String\n  },\n  data() {\n    return {\n      showDialog: false,\n      // Controlează vizibilitatea dialogului\n      currentPackage: {\n        deliveryAddress: \"\",\n        status: \"PENDING\",\n        // Valoare implicită pentru status\n        courier: {\n          name: \"\",\n          email: \"\",\n          payOnDelivery: false // Valoare implicită\n        }\n      }\n    };\n  },\n  methods: {\n    async createPackage() {\n      try {\n        // Trimite datele către server\n        const response = await axios.post(\"http://localhost:8083/package\", this.currentPackage);\n        console.log(\"Package created successfully:\", response.data);\n\n        // Trimite noul pachet către `PackagesView` printr-un eveniment\n        this.$emit(\"package-created\", response.data);\n\n        // Închide dialogul\n        this.closeDialog();\n      } catch (error) {\n        console.error(\"Error creating package:\", error);\n      }\n    },\n    closeDialog() {\n      this.showDialog = false;\n\n      // Resetează formularul\n      this.currentPackage = {\n        deliveryAddress: \"\",\n        status: \"PENDING\",\n        courier: {\n          name: \"\",\n          email: \"\",\n          payOnDelivery: false\n        }\n      };\n\n      // Notifică componenta părinte că dialogul a fost închis\n      this.$emit(\"dialog-closed\");\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","currentUser","String","data","showDialog","currentPackage","deliveryAddress","status","courier","email","payOnDelivery","methods","createPackage","response","post","console","log","$emit","closeDialog","error"],"sources":["D:\\proiectSCD\\packagetracking\\packagetracking\\packagetracking-frontend\\src\\components\\AddPackage.vue"],"sourcesContent":["<template>\r\n  <v-dialog v-model=\"showDialog\" max-width=\"400px\" class=\"bg-transparent\">\r\n    <div class=\"dialogClass\">\r\n      <h3>Welcome, {{ currentUser }}! Let's create a new package!</h3>\r\n\r\n      <!-- Delivery Address -->\r\n      <v-textarea\r\n          label=\"Delivery Address\"\r\n          v-model=\"currentPackage.deliveryAddress\"\r\n      ></v-textarea>\r\n\r\n      <!-- Status -->\r\n      <v-text-field\r\n          label=\"Status\"\r\n          v-model=\"currentPackage.status\"\r\n      ></v-text-field>\r\n\r\n      <!-- Courier Name -->\r\n      <v-text-field\r\n          label=\"Courier Name\"\r\n          v-model=\"currentPackage.courier.name\"\r\n      ></v-text-field>\r\n\r\n      <!-- Courier Email -->\r\n      <v-text-field\r\n          label=\"Courier Email\"\r\n          v-model=\"currentPackage.courier.email\"\r\n      ></v-text-field>\r\n\r\n      <!-- Pay on Delivery -->\r\n      <v-checkbox\r\n          label=\"Pay On Delivery\"\r\n          v-model=\"currentPackage.courier.payOnDelivery\"\r\n      ></v-checkbox>\r\n\r\n      <v-btn color=\"green\" @click=\"createPackage\">Create</v-btn>\r\n      <v-btn color=\"red\" @click=\"closeDialog\">Cancel</v-btn>\r\n    </div>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"AddPackage\",\r\n  props: {\r\n    currentUser: String,\r\n  },\r\n  data() {\r\n    return {\r\n      showDialog: false, // Controlează vizibilitatea dialogului\r\n      currentPackage: {\r\n        deliveryAddress: \"\",\r\n        status: \"PENDING\", // Valoare implicită pentru status\r\n        courier: {\r\n          name: \"\",\r\n          email: \"\",\r\n          payOnDelivery: false, // Valoare implicită\r\n        },\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async createPackage() {\r\n      try {\r\n        // Trimite datele către server\r\n        const response = await axios.post(\r\n            \"http://localhost:8083/package\",\r\n            this.currentPackage\r\n        );\r\n        console.log(\"Package created successfully:\", response.data);\r\n\r\n        // Trimite noul pachet către `PackagesView` printr-un eveniment\r\n        this.$emit(\"package-created\", response.data);\r\n\r\n        // Închide dialogul\r\n        this.closeDialog();\r\n      } catch (error) {\r\n        console.error(\"Error creating package:\", error);\r\n      }\r\n    },\r\n    closeDialog() {\r\n      this.showDialog = false;\r\n\r\n      // Resetează formularul\r\n      this.currentPackage = {\r\n        deliveryAddress: \"\",\r\n        status: \"PENDING\",\r\n        courier: {\r\n          name: \"\",\r\n          email: \"\",\r\n          payOnDelivery: false,\r\n        },\r\n      };\r\n\r\n      // Notifică componenta părinte că dialogul a fost închis\r\n      this.$emit(\"dialog-closed\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.dialogClass {\r\n  padding: 20px;\r\n  background-color: white;\r\n}\r\n</style>\r\n"],"mappings":"AA0CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,WAAW,EAAEC;EACf,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK;MAAE;MACnBC,cAAc,EAAE;QACdC,eAAe,EAAE,EAAE;QACnBC,MAAM,EAAE,SAAS;QAAE;QACnBC,OAAO,EAAE;UACPT,IAAI,EAAE,EAAE;UACRU,KAAK,EAAE,EAAE;UACTC,aAAa,EAAE,KAAK,CAAE;QACxB;MACF;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF;QACA,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,IAAI,CAC7B,+BAA+B,EAC/B,IAAI,CAACT,cACT,CAAC;QACDU,OAAO,CAACC,GAAG,CAAC,+BAA+B,EAAEH,QAAQ,CAACV,IAAI,CAAC;;QAE3D;QACA,IAAI,CAACc,KAAK,CAAC,iBAAiB,EAAEJ,QAAQ,CAACV,IAAI,CAAC;;QAE5C;QACA,IAAI,CAACe,WAAW,CAAC,CAAC;MACpB,EAAE,OAAOC,KAAK,EAAE;QACdJ,OAAO,CAACI,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACjD;IACF,CAAC;IACDD,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACd,UAAS,GAAI,KAAK;;MAEvB;MACA,IAAI,CAACC,cAAa,GAAI;QACpBC,eAAe,EAAE,EAAE;QACnBC,MAAM,EAAE,SAAS;QACjBC,OAAO,EAAE;UACPT,IAAI,EAAE,EAAE;UACRU,KAAK,EAAE,EAAE;UACTC,aAAa,EAAE;QACjB;MACF,CAAC;;MAED;MACA,IAAI,CAACO,KAAK,CAAC,eAAe,CAAC;IAC7B;EACF;AACF,CAAC","ignoreList":[]},"me